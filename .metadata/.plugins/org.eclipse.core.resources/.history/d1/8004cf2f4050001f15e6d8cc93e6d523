package practices1;

public class RemoveDuplicateInt {

	public static void main(String[] args) {
		int[]nums= {0,1,1,2,2,2};
		removeDuplicates(nums);
		
		

	}
	public static int removeDuplicates(int[] nums) {
		int a=0;
		for(int i=0;i<nums.length;i++) {
			if(nums[i]==nums[i+1]) {
				nums[i]=0;
				
			}
			
			if(nums[i]!=nums[i+1]) {
				a++;
			}
		}
		return a;
        
    }

}
